@model List<UserGetModel>
@{
    ViewData["Title"] = "Users";
}

   <table class="table">
       <thead>
           <tr>
               <th scope="col">Firstname</th>
               <th scope="col">Lastname</th>
               <th scope="col">Id Number</th>
               <th scope="col">BirthDate</th>
               <th scope="col">Salary</th>
               <th scope="col">BirthCertificateId</th>
               <th scope="col">Address</th>
               <th scope="col">Age</th>
               <th scope="col">Average Salary</th>
           </tr>
       </thead>
       <tbody>
           @foreach (var item in Model)
           {
               <tr>
                   <td>@item.Firstname</td>
                   <td>@item.Lastname</td>
                   <td>@item.IdNumber</td>
                   <td>@item.BirthDate</td>
                   <td>@item.Salary</td>
                   <td>@item.BirthCertificateId</td>
                   <td>@item.Address</td>
                   <td>@item.Age</td>
                   <td>@item.AverageSalary</td>
               </tr>
           }
       </tbody>
   </table>
 
